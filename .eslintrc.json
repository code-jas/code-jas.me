{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
   "plugins": ["prettier"],
   "rules": {
      "prettier/prettier": [
         "error",
         {
            "semi": true,
            "singleQuote": true,
            "tabWidth": 3,
            "trailingComma": "all",
            "endOfLine": "auto",
            "printWidth": 100
         }
      ]
   }
}
